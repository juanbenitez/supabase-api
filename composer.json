{
    "name": "juanbenitez/supabase-api",
    "description": "Minimal PHP implementation of the Supabase API.",
    "keywords": [
        "juanbenitez",
        "supabase-api"
    ],
    "homepage": "https://github.com/juanbenitez/supabase-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Benitez",
            "email": "juan.angel.benitez@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "sammyjo20/saloon": "^1.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Juanbenitez\\SupabaseApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juanbenitez\\SupabaseApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
